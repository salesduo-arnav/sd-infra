{
    "name": "template-backend",
    "version": "1.0.0",
    "scripts": {
        "dev": "npx ts-node scripts/wait-for-db.ts && npm run migrate:up && nodemon",
        "build": "tsc",
        "start": "npm run migrate:up && node dist/server.js",
        "lint": "eslint src",
        "clean": "rimraf dist && rimraf node_modules",
        "migrate:create": "npx sequelize-cli migration:generate",
        "migrate:up": "npx sequelize-cli db:migrate",
        "migrate:down": "npx sequelize-cli db:migrate:undo",
        "test": "cross-env PGDATABASE=mydb_test jest"
    },
    "dependencies": {
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "helmet": "^7.1.0",
        "pg": "^8.17.2",
        "pg-hstore": "^2.3.4",
        "sequelize": "^6.37.7"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.2",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jest": "^30.0.0",
        "@types/node": "^20.10.0",
        "@types/pg": "^8.16.0",
        "@types/sequelize": "^4.28.20",
        "@types/supertest": "^6.0.3",
        "cross-env": "^10.1.0",
        "eslint": "^9.39.2",
        "globals": "^17.1.0",
        "jest": "^30.2.0",
        "nodemon": "^3.0.2",
        "rimraf": "^5.0.5",
        "sequelize-cli": "^6.6.5",
        "supertest": "^7.2.2",
        "ts-jest": "^29.4.6",
        "ts-node": "^10.9.1",
        "typescript": "^5.3.2",
        "typescript-eslint": "^8.53.1"
    }
}